<?xml version = "1.0" encoding = "utf-8"?>
<resources>
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="indicatorColor" format="color" />
        <attr name="underlineColor" format="color" />
        <attr name="dividerColor" format="color" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="underlineHeight" format="dimension" />
        <attr name="tabPaddingLeftRight" format="dimension" />
        <attr name="scrollOffset" format="dimension" />
        <attr name="tabBackground" format="reference" />
        <attr name="shouldExpand" format="boolean" />
        <attr name="tabTextSize" format="dimension" />
        <attr name="tabTextColor" format="color" />
        <attr name="TabStripDividerPadding" format="dimension"/>
        <attr name="TabStripTextAllCaps" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>

    <declare-styleable name="ExploreCircleProgressView">
        <attr name="progressText" format="string"/>
        <attr name="maxText" format="string" />
        <attr name="upperMinorText" format="string" />
        <attr name="lowerMinorText" format="string" />
        <attr name="min" format="integer" />
        <attr name="max" format="integer" />
        <attr name="progress" format="float" />
        

    </declare-styleable>
</resources>